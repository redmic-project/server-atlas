{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Layer Info DTO",
	"type": "object",
	"properties": {
		"id": {
			"type": ["string", "null"]
		},
		"themeInspire": {
			"type": ["integer", "null"],
			"url": "/api/atlas/commands/themeinspire"
		},
		"latLonBoundsImage": {
			"$ref": "#/definitions/LatLonBoundingBoxDTO"
		},
		"protocols": {
			"type": "array",
			"uniqueItemsByRequiredProperties": true,
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "#/definitions/ProtocolDTO"
			}
		},
		"description": {
			"type": ["string", "null"],
			"maxLength": 1500
		},
		"alias": {
			"type": ["string", "null"],
			"maxLength": 250
		},
		"atlas": {
			"type": "boolean",
			"default": "false"
		},
		"refresh": {
			"type": "integer",
			"default": "0"
		},
		"parent": {
			"type": ["integer", "null"],
			"url": "/api/atlas/commands/category"
		}
	},
	"required": ["protocols", "atlas", "refresh"],
	"definitions": {
		"LatLonBoundingBoxDTO": {
			"type": ["object", "null"],
			"properties": {
				"minX": {
					"type": "number"
				},
				"minY": {
					"type": "number"
				},
				"maxX": {
					"type": "number"
				},
				"maxY": {
					"type": "number"
				}
			},
			"required": ["minX", "minY", "maxX", "maxY"]
		},
		"ProtocolDTO": {
			"type": "object",
			"properties": {
				"type": {
					"type": "string"
				},
				"url": {
					"type": "string",
					"format": "url"
				}
			},
			"required": ["type", "url"]
		}
	}
}